{
    "name": "axios-ex",
    "description": "axios extend",
    "version": "2.3.0-beta.2",
    "packageManager": "pnpm@7.7.0",
    "main": "lib/index.cjs",
    "module": "lib/index.mjs",
    "unpkg": "lib/index.min.js",
    "exports": {
        ".": {
            "require": "./lib/index.cjs",
            "import": "./lib/index.mjs"
        },
        "./lib": "./lib/index.cjs",
        "./*": "./*"
    },
    "files": [
        "es",
        "lib",
        "typings"
    ],
    "directories": {
        "es": "es",
        "lib": "lib",
        "src": "src"
    },
    "scripts": {
        "build": "rimraf lib es && rollup -c",
        "build:umd": "webpack --config webpack.config.js",
        "build:types": "run-s build-temp-types roll-types",
        "build-temp-types": "tsc --emitDeclarationOnly --outDir temp/ -p src/",
        "patch-types": "node script/patchTypes",
        "roll-types": "api-extractor run && rimraf temp",
        "dev": "rollup -c -w",
        "pub": "tscjs scripts/publish",
        "unpub": "tscjs scripts/unpublish",
        "sync": "tscjs scripts/sync",
        "dist": "run-s eslint prettier build build:umd build:types docs prettier:docs",
        "test-unit": "jest --filter ./scripts/filter-unit.js",
        "jest": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest --coverage",
        "coveralls": "cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
        "docs": "rimraf docs && typedoc",
        "eslint": "eslint --fix .",
        "prettier": "prettier --write \"**/*.{js,ts,jsx,tsx,yml,json,md}\"",
        "prettier:docs": "prettier --write \"**/*.md\""
    },
    "dependencies": {
        "axios": "0.27.2",
        "core-decorators": "^0.20.0",
        "core-js": "^3.24.1",
        "is-retry-allowed": "^3.0.0",
        "js-cool": "^2.3.2",
        "regenerator-runtime": "^0.13.9"
    },
    "devDependencies": {
        "@babel/cli": "^7.18.9",
        "@babel/core": "^7.18.9",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-decorators": "^7.18.9",
        "@babel/preset-env": "^7.18.9",
        "@babel/preset-typescript": "^7.18.6",
        "@eslint-sets/eslint-config-simple-ts": "^3.0.4",
        "@microsoft/api-extractor": "^7.28.6",
        "@rollup/plugin-babel": "^5.3.1",
        "@rollup/plugin-commonjs": "^22.0.1",
        "@rollup/plugin-node-resolve": "^14.0.0",
        "@types/jest": "^29.0.0",
        "@types/node": "^18.6.2",
        "babel-loader": "^8.2.5",
        "coveralls": "^3.1.1",
        "cross-env": "^7.0.3",
        "esbuild": "^0.15.5",
        "eslint": "^8.20.0",
        "eslint-config-sets": "^1.8.3",
        "jest": "^28.1.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.7.1",
        "prettier-config-common": "^1.2.0",
        "progress-bar-webpack-plugin": "^2.1.0",
        "rimraf": "^3.0.2",
        "rollup": "^2.77.2",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.33.0",
        "rollup-plugin-visualizer": "^5.7.1",
        "terser-webpack-plugin": "^5.3.3",
        "ts-jest": "^28.0.7",
        "tsnd": "^1.0.2",
        "typedoc": "^0.23.9",
        "typedoc-plugin-markdown": "^3.13.4",
        "typescript": "^4.7.4",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-merge": "^5.8.0",
        "webpack-node-externals": "^3.0.0"
    },
    "keywords": [
        "axios",
        "plugin",
        "extension",
        "Promise",
        "axios-ex",
        "axios-plugin",
        "axios-extend",
        "saqqdy"
    ],
    "license": "MIT",
    "author": "saqqdy@qq.com",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/saqqdy/axios-ex.git"
    },
    "bugs": {
        "url": "https://github.com/saqqdy/axios-ex/issues"
    },
    "pnpm": {
        "peerDependencyRules": {
            "ignoreMissing": [
                "@types/react",
                "react",
                "react-dom",
                "@algolia/client-search",
                "tslib",
                "webpack"
            ],
            "allowedVersions": {
                "eslint": "^8.0.0"
            }
        }
    },
    "homepage": "https://github.com/saqqdy/axios-ex#readme",
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    }
}
