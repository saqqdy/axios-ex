{
    "name": "axios-ex",
    "version": "2.2.4",
    "description": "axios extend",
    "main": "lib/index.js",
    "unpkg": "lib/index.js",
    "module": "lib/index.esm.js",
    "files": [
        "es",
        "lib",
        "typings"
    ],
    "directories": {
        "es": "es",
        "lib": "lib",
        "src": "src"
    },
    "scripts": {
        "build": "rimraf lib es && rollup -c",
        "build:umd": "webpack --config webpack.config.js",
        "build:types": "run-s build-temp-types roll-types",
        "build-temp-types": "tsc --emitDeclarationOnly --outDir temp/ -p src/",
        "patch-types": "node script/patchTypes",
        "roll-types": "api-extractor run && rimraf temp",
        "dev": "rollup -c -w",
        "dist": "run-s eslint prettier build build:umd build:types docs prettier:docs",
        "test-unit": "jest --filter ./scripts/filter-unit.js",
        "jest": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest --coverage",
        "coveralls": "cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
        "docs": "rimraf docs && typedoc",
        "eslint": "eslint --fix --ext .ts,.js",
        "prettier": "prettier --write \"**/*.{js,ts,json,md}\"",
        "prettier:docs": "prettier --write \"**/*.md\""
    },
    "dependencies": {
        "axios": "0.27.2",
        "core-decorators": "^0.20.0",
        "core-js": "^3.22.3",
        "is-retry-allowed": "^3.0.0",
        "js-cool": "^2.3.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.10",
        "@babel/core": "^7.17.10",
        "@babel/eslint-parser": "^7.17.0",
        "@babel/plugin-proposal-class-properties": "^7.16.7",
        "@babel/plugin-proposal-decorators": "^7.17.9",
        "@babel/plugin-transform-runtime": "^7.17.10",
        "@babel/preset-env": "^7.17.10",
        "@babel/preset-typescript": "^7.16.7",
        "@eslint-sets/eslint-config-simple-ts": "^2.1.4",
        "@microsoft/api-extractor": "^7.23.0",
        "@rollup/plugin-babel": "^5.3.1",
        "@rollup/plugin-commonjs": "^22.0.0",
        "@rollup/plugin-node-resolve": "^13.2.1",
        "@saqqdy/prettier-config": "^1.1.2",
        "@types/jest": "^27.4.1",
        "@types/node": "^17.0.30",
        "@typescript-eslint/eslint-plugin": "^5.21.0",
        "@typescript-eslint/parser": "^5.21.0",
        "babel-loader": "^8.2.5",
        "coveralls": "^3.1.1",
        "cross-env": "^7.0.3",
        "esbuild": "^0.14.38",
        "eslint": "^8.14.0",
        "eslint-config-sets": "^1.8.2",
        "jest": "^28.0.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.6.2",
        "progress-bar-webpack-plugin": "^2.1.0",
        "rimraf": "^3.0.2",
        "rollup": "^2.71.1",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.31.2",
        "rollup-plugin-visualizer": "^5.6.0",
        "terser-webpack-plugin": "^5.3.1",
        "ts-jest": "^27.1.4",
        "tsnd": "^1.0.2",
        "typedoc": "^0.22.15",
        "typedoc-plugin-markdown": "^3.12.1",
        "typescript": "^4.6.4",
        "webpack": "^5.72.0",
        "webpack-cli": "^4.9.2",
        "webpack-merge": "^5.8.0",
        "webpack-node-externals": "^3.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/saqqdy/axios-ex.git"
    },
    "keywords": [
        "axios",
        "plugin",
        "extension",
        "Promise",
        "axios-ex",
        "axios-plugin",
        "axios-extend",
        "saqqdy"
    ],
    "author": "saqqdy@qq.com",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/saqqdy/axios-ex/issues"
    },
    "pnpm": {
        "peerDependencyRules": {
            "ignoreMissing": [
                "@types/react",
                "react",
                "react-dom",
                "@algolia/client-search",
                "tslib",
                "webpack"
            ],
            "allowedVersions": {
                "eslint": "^8.10.0"
            }
        }
    },
    "homepage": "https://github.com/saqqdy/axios-ex#readme"
}
