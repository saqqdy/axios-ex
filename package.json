{
	"name": "axios-ex",
	"description": "A integration solution of axios for large enterprise projects",
	"version": "3.0.0-beta.1",
	"packageManager": "pnpm@7.26.1",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"unpkg": "./dist/index.min.js",
	"jsdelivr": "./dist/index.min.js",
	"typings": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		},
		"./*": "./*"
	},
	"files": [
		"dist",
		"es",
		"lib"
	],
	"directories": {
		"dist": "dist",
		"es": "es",
		"lib": "lib",
		"src": "src"
	},
	"scripts": {
		"build": "run-s clean build:{bundle,types,docs} roll-types",
		"build:bundle": "rollup -c build/rollup.config.ts --configPlugin @rollup/plugin-typescript",
		"build:umd": "webpack --config build/webpack.config.js",
		"build:types": "tsc --emitDeclarationOnly --outDir temp -p src",
		"build:docs": "rimraf docs && typedoc",
		"roll-types": "api-extractor run && rimraf temp",
		"watch": "pnpm build:bundle -w",
		"pub": "tscjs scripts/publish",
		"unpub": "tscjs scripts/unpublish",
		"sync": "tscjs scripts/sync",
		"clean": "rimraf dist",
		"dist": "run-s clean build prettier:docs",
		"test-unit": "jest --filter ./scripts/filter-unit.js",
		"jest": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest --coverage",
		"coveralls": "cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
		"eslint": "eslint --fix .",
		"prettier": "prettier --write \"**/*.{js,ts,jsx,tsx,yml,json,md}\"",
		"prettier:docs": "prettier --write \"**/*.md\""
	},
	"dependencies": {
		"axios": "^1.2.5",
		"core-js": "^3.27.2",
		"is-retry-allowed": "^3.0.0",
		"js-cool": "^2.8.0",
		"regenerator-runtime": "^0.13.11"
	},
	"devDependencies": {
		"@babel/core": "^7.20.12",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-typescript": "^7.18.6",
		"@eslint-sets/eslint-config-ts": "^5.2.0",
		"@microsoft/api-extractor": "^7.34.0",
		"@rollup/plugin-alias": "^4.0.3",
		"@rollup/plugin-babel": "^6.0.3",
		"@rollup/plugin-commonjs": "^24.0.1",
		"@rollup/plugin-json": "^6.0.0",
		"@rollup/plugin-node-resolve": "^15.0.1",
		"@rollup/plugin-terser": "^0.4.0",
		"@rollup/plugin-typescript": "^11.0.0",
		"@types/jest": "^29.4.0",
		"@types/node": "^18.11.18",
		"babel-loader": "^9.1.2",
		"chalk": "^5.2.0",
		"coveralls": "^3.1.1",
		"cross-env": "^7.0.3",
		"esbuild": "^0.17.4",
		"eslint": "^8.32.0",
		"fast-glob": "^3.2.12",
		"jest": "^29.4.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.3",
		"prettier-config-common": "^1.3.0",
		"progress-bar-webpack-plugin": "^2.1.0",
		"reinstaller": "^2.1.0",
		"rimraf": "^4.1.2",
		"rollup": "^3.11.0",
		"rollup-plugin-filesize": "^9.1.2",
		"rollup-plugin-visualizer": "^5.9.0",
		"terser-webpack-plugin": "^5.3.6",
		"ts-jest": "^29.0.5",
		"ts-morph": "^17.0.1",
		"tsnd": "^1.1.0",
		"typedoc": "^0.23.24",
		"typedoc-plugin-markdown": "^3.14.0",
		"typescript": "^4.9.4",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1",
		"webpack-merge": "^5.8.0",
		"webpack-node-externals": "^3.0.0"
	},
	"peerDependencies": {
		"axios": ">= 1",
		"core-js": ">= 3",
		"js-cool": ">= 2"
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"tslib",
				"webpack"
			],
			"allowedVersions": {
				"eslint": "^8.0.0"
			}
		}
	},
	"keywords": [
		"axios",
		"plugin",
		"extension",
		"Promise",
		"axios-ex",
		"axios-plugin",
		"axios-extend",
		"saqqdy"
	],
	"license": "MIT",
	"author": "saqqdy <https://github.com/saqqdy>",
	"homepage": "https://github.com/saqqdy/axios-ex#readme",
	"bugs": {
		"url": "https://github.com/saqqdy/axios-ex/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/saqqdy/axios-ex.git"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"access": "public"
	}
}
